	public static void main(String[] args) {
		doReg();
	}
	public static void doReg() {
		Scanner scanner = new Scanner(System.in);
		do {
			System.out.println("press 1 for registration");
			System.out.println("press 2 for login");
		} while(true);
		
	}
}
